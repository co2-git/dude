[
  {
    "name": "MongoDB",
    "slug": "mongodb",
    "about": "MongoDB (from \"humongous\") is an open-source document database, and the leading NoSQL database. ",
    "tags": ["mongodb", "mongo", "database", "db", "nosql", "json", "document", "opensource"],
    "latest": "2.6.1",
    "versions": ["2.6.1"],
    "install": {
      "2.6.1": {
        "source": "http://fastdl.mongodb.org/{{os.type}}/mongodb-{{os.type}}-{{os.arch}}-{{dependency.version}}.tgz",
        "extension": "tgz"
      }
    },
    "start": {
      "2.6.1": {
        "script": "bin/mongod",
        "arguments-style": {},
        "arguments": {
          "dbpath": {
            "about": "Path to the database",
            "required": false
          },
          "port": {
            "about": "Port to listen to",
            "required": false
          }
        },
        "arguments-transform": "object to long options"
      }
    }
  }
]